CREATE TABLE hbie_tenfp
(
  ID    VARCHAR2(32) PRIMARY KEY,
  VER   NUMBER(3,0),
  NAME  varchar2(100),
  PATH  varchar2(100),
  DIR   varchar2(100),
  FEA0  BLOB,
  FEA1  BLOB,
  FEA2  BLOB,
  FEA3  BLOB,
  FEA4  BLOB,
  FEA5  BLOB,
  FEA6  BLOB,
  FEA7  BLOB,
  FEA8  BLOB,
  FEA9  BLOB,
  CREATE_TIME TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);

CREATE INDEX IDX_TENFP_ID_VER ON hbie_tenfp(ID,VER,DIR,NAME);